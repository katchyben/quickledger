<odoo>
  <record id="ledger_entry_wizard" model="ir.ui.view">
    <field name="name">Ledger Entry Wizard</field> 
    <field name="model">ledger.entry.wizard</field> 
    <field name="arch" type="xml"> 
      <form>
        <group>
            <field name="session_id" widget="selection" />
            <field name="payment_id"  widget="selection"/>
            <field name="faculty_id"  widget="selection"/>
        </group>
        <footer>
          <button type="object" name="print_report" string="Print" class="oe_highlight"/>
          <button special="cancel" string="Cancel"/>
        </footer>
      </form>
    </field>
  </record>

  <act_window id="action_ledger_entry_report_wizard"
              name="Fees By Name"
              res_model="ledger.entry.wizard"
              view_mode="form"
              target="new"/>
</odoo>






